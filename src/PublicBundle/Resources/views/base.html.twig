    <section class="contactInfos">
      <div class="container">
        <div class="row">
          <div class="span12">
            <p id="flags" class="pull-left">
				{% set locale = app.request.locale %}

				{% if locale == 'es' %}
				<img src="/img/banderas/spain.png" alt="Español">
				{% else %}
				<a href="http://www.furgomania.com/es/"><img src="/img/banderas/spain.png" alt="Español"></a>
				{% endif %}

				{% if locale == 'en' %}
				<img src="/img/banderas/uk.png" alt="English">
				{% else %}
				<a href="http://www.furgomania.com/en/"><img src="/img/banderas/uk.png" alt="English"></a>
				{% endif %}

				{% if locale == 'fr' %}
				<img src="/img/banderas/france.png" alt="Français">
				{% else %}
				<a href="http://www.furgomania.com/fr/"><img src="/img/banderas/france.png" alt="Français"></a>
				{% endif %}

				{% if locale == 'pt' %}
				<img src="/img/banderas/portugal.png" alt="Português">
				{% else %}
				<a href="http://www.furgomania.com/pt/"><img src="/img/banderas/portugal.png" alt="Português"></a>
				{% endif %}

				{% if locale == 'de' %}
				<img src="/img/banderas/germany.png" alt="Deutsch">
				{% else %}
				<a href="http://www.furgomania.com/de/"><img src="/img/banderas/germany.png" alt="Deutsch"></a>
				{% endif %}
            </p>

			<p class="pull-right">
				<a href="https://www.facebook.com/Furgomania/" target="_blank"><img src="/img/social/facebook.png" alt="Facebook"></a>
				<a href="https://www.youtube.com/c/furgomania" target="_blank"><img src="/img/social/youtube.png" alt="Youtube"></a>
				<a href="https://plus.google.com/+furgomania" target="_blank"><img src="/img/social/google.png" alt="Google+"></a>
				<a href="https://twitter.com/furgomania" target="_blank"><img src="/img/social/twitter.png" alt="Twitter"></a>
				
				{#<div class="addthis_sharing_toolbox" style="width:96px; float:right; padding-top:5px"></div>#}
			</p>

          </div>

        </div>
      </div>
